<script>


  function deleteorder(uid) {
    console.log("89")

    $.ajax({
      url: ('/cancelorder/' + uid),

      method: 'get',

      success: (response) => {


        location.reload()
      }


    })
  }






  function Hello(uid) {
    swal({
      title: "Are you sure?",
      text: "Once Cancelled, you will not be able to recover this Order!",
      icon: "warning",
      buttons: true,
      dangerMode: true,
    })
      .then((willDelete) => {
        if (willDelete) {
          deleteorder(uid)
          swal("Poof! Info has been deleted!", {
            icon: "success",
          });
        } else {
          swal("Your  file is safe!");
        }
      });
  }







</script>
